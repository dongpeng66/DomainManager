
source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '12.0'
# 用来忽略pod里面的警告用的
inhibit_all_warnings!

def commen
   pod 'BlueSocket'
  
end


target 'DomainManager' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
   use_modular_headers!
   
   commen

end




